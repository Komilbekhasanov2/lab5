#include <iostream>
using namespace std;
int main() {
    int number;
    cout << "Enter a number: ";
    cin >> number;
    for (int i = 1; i <= number; i++) {
        if (number!=0){
            for (int j = 1; j <= i; j++) {
                cout << i;
            }
        }
        else{
            cout << "The depth is 0";
        }
        cout << endl;
    }
    return 0;
}
